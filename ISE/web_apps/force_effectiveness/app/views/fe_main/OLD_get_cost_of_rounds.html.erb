{
	"series":
		[
			<% first_record = true %>
			<% x_axis_ticks = "" %>
			<% x_axis_max = 0 %>
			
			<% x_axis_interval = 5 %>
			<% x_axis_labels = @batteries %>
			
			
  			<% @track_type_battery_hash.each_pair do |track_type, battery_array| %>
				
				{ "data": [
					<% x_axis_array = battery_array %>
					<% i = 0 %>
					<% battery_array.each do |value| %>
					
						[<%= i * x_axis_interval %>, <%=h value%>],
						
						<% if first_record 
						
							x_axis_ticks = x_axis_ticks + '[' + (i*x_axis_interval).to_s + ', "' + x_axis_labels[i] + '"],'							
						
						   end %>
						   
						<% i = i + 1 %>
						
					<% end %>
					
				], "label": "<%= @series_name %>", "xaxis": 1},
				
				<% first_record = false%>
				<% x_axis_max = i*5 %>
			<% end %>
		], 
	"options": 
		{ 
			"title": "Cumulative Cost of Rounds Expended",
			"bars": {"show": true, "barWidth": 1, "centered": true, "stacked": true},
			"yaxis": {"noTicks": 5, "showLabels": true, "autoScaleMargin": 0, "min": 0, "max": 300},
			"xaxis": {"show": true, "noTicks": 5, "min": 0, "max": <%= x_axis_max.to_s %>, "ticks": [<%= x_axis_ticks %>]}
			
		}    
}